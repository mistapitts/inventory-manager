# Supabase Environment Variables Template
# Copy this file to .env.local for local development
# Add these to Vercel Environment Variables for production

# Your Supabase project URL (from Settings > API)
SUPABASE_URL=https://your-project-id.supabase.co

# Your Supabase anon/public key (from Settings > API)
SUPABASE_ANON_KEY=your-anon-key-here

# JWT secret for authentication (can be any secure string)
JWT_SECRET=your-secure-jwt-secret-here

# Optional: Set to 'production' for production builds
NODE_ENV=development
